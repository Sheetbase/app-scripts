var fs = require('fs-extra');
var path = require('path');

(function () {
    const dist = path.resolve('.', 'dist');
    fs.copySync('package.json', dist + '/package.json');
    fs.copySync('README.md', dist + '/README.md');
    fs.copySync('LICENSE', dist + '/LICENSE');
    console.log('Copied!');
})();